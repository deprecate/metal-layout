{namespace Layout}

/**
* This renders the component's whole content.
* @param data
*/
{template .render}
	<div class="layout">
			{foreach $value in ($data)}
				<div class="row">
					{foreach $column in ($value.columns)}
						<div class="col-md-{$column.size}">{$column.content}</div>
					{/foreach}
				</div>
			{/foreach}
	</div>
{/template}